<table>
  <tr>
    <th>Name</th>
    <th>Price</th>
  </tr>

<% @car_options.each do |car_option| %>
  <tr>
    <td><%= check_box("car_option", "car_option", checked_value: car_option.token, :class => 'car_option_ops  car_ops') %><%= car_option.name %></td>
    <td><%= car_option.price %></td>
  </tr>
<% end %>
</table>